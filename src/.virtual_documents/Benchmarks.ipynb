for name, dataset in datasets.items():
    cnt = 0 
    avg_gurobi_reach = 0 
    avg_gurobi_nodes = 0
    avg_gurobi_time = 0
    avg_cplex_reach = 0
    avg_cplex_nodes = 0
    avg_cplex_time = 0
